@model PlantItOut.ViewModels.SearchPlantViewModel

<h1>Delete Tag</h1>

<form method="post">
    @foreach (Tag tag in Model.Tags)
    {
        <div class="form-group">
            <span>@tag.Name</span>
            <input type="checkbox" name="tagId" value="@tag.Id" class="form-control">
        </div>
    }
</form>